<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<template id="kojto_pdf_header_css_assets">
		<style>
            body, table, tbody td { font-family: 'Roboto', sans-serif; font-size: 10px; }
            p { margin: 10px 0px; }
            .content-table th, .content-table td { padding: 4px; text-align: left; border-top: none; border-bottom: 0.5px solid gray; border-left: none; border-right: none; }
            .content-table { width: 100%; border-collapse: collapse; border-top: 0.5px solid gray; border-bottom: 0.5px solid gray; margin-bottom: 20px; font-size: 12px; }
            .even-row { background-color: #f0f0f0; border-bottom: 0.5px solid gray; }
            .footer-section { margin-top: 30px; text-align: center; }
            .footer-section p { margin: 0; font-size: 12px; }
            .header-row { background-color: #d3d3d3; }
            .heading { text-transform: uppercase; font-size: 16px; font-weight: bold; margin: 0 0 3mm 0; }
            .info-cell { width: 45%; font-family: 'Roboto', sans-serif; font-size: 12px; }
            .info-cell div { margin: 0 0 1mm 0; }
            .info-section { display: flex; justify-content: space-between; font-family: 'Roboto', sans-serif; font-size: 12px; margin-bottom: 10px;}
            .odd-row { background-color: white; border-bottom: 0.5px solid gray; }
            .section-heading { margin: 5mm auto 5mm; font-size: 14px; text-align: center; }
            .summary-section { margin-top: 30px; text-align: right; }
            .content-table .text-right { text-align: right; }

			.ko-w-10 { width: 10px; }
			.ko-w-20 { width: 20px; }
			.ko-w-30 { width: 30px; }
			.ko-w-40 { width: 40px; }
			.ko-w-50 { width: 50px; }
			.ko-w-60 { width: 60px; }
			.ko-w-70 { width: 70px; }
			.ko-w-80 { width: 80px; }
			.ko-w-90 { width: 90px; }
			.ko-w-100 { width: 100px; }
			.ko-w-110 { width: 110px; }
			.ko-w-120 { width: 120px; }
			.ko-w-130 { width: 130px; }
			.ko-w-140 { width: 140px; }
			.ko-w-150 { width: 150px; }
			.ko-w-160 { width: 160px; }
			.ko-w-170 { width: 170px; }
			.ko-w-180 { width: 180px; }
			.ko-w-190 { width: 190px; }
			.ko-w-200 { width: 200px; }

            @page {size: A4; margin: 25mm 10mm 10mm 10mm; padding: 5mm 0; background: white; color: black; font-family: 'Roboto', sans-serif; font-size: 10px;
            @top-left-corner { content: ''; text-align: center; }
            @top-left { content: ''; text-align: left; }
            @top-center { content: ''; text-align: center; }
            @top-right { width: 80px; vertical-align: bottom; padding-top: 20px; height: 100%; content:            url('data:image/svg+xml;base64,');
            }
            @top-right-corner { content: ''; text-align: center; }
            @left-top { content: ''; text-align: center; }
            @left-middle { content: ''; text-align: center; }
            @left-bottom { content: ''; text-align: center; }
            @bottom-left-corner { content: ''; text-align: center; }
            @bottom-left { content: 'www.kojto.com'; text-align: left; vertical-align: top; padding-top: 10px}
            @bottom-center { content: ' '; text-align: center; vertical-align: top; padding-top: 10px}
            @bottom-right { content: "page " counter(page) " / " counter(pages); text-align: right; vertical-align: top; padding-top: 10px}
            @bottom-right-corner { content: ''; text-align: center; }
            @right-top { content: ''; text-align: center; }
            @right-middle { content: ''; text-align: center; }
            @right-bottom { content: ''; text-align: center; }
            }
        </style>
	</template>
</odoo>
